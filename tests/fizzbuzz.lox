# like loopTest but better

fun fizzbuzz(start, stop, step) {
    for (var i = start; i <= stop; i = i + step) {
        if (i % 5 == 0 and i % 3 == 0)
            puts('fizzbuzz');
        else if (i % 5 == 0)
            puts('buzz');
        else if (i % 3 == 0)
            print('fizz');
        else
            print(i);
    }
}

